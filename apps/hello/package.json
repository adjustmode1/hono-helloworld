{
  "name": "@halomeapis/hello",
  "private": true,
  "version": "3.5.64-alpha.0",
  "description": "Hono TypeScript starter repository",
  "license": "MIT",
  "engines": {
    "node": ">=18 <19"
  },
  "scripts": {
    "build-type": ".bin/build-types.sh",
    "prebuild": "rimraf dist",
    "build": "rimraf dist && tsc",
    "dev": "tsc-watch --onSuccess \"node dist/main.js\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "ts-patch install && typia patch && [ \\\"$npm_config_production\\\" ] && exit 0; husky install",
    "test": "jest",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.13",
    "@hono/swagger-ui": "^0.5.0",
    "@packages/cloudevents": "workspace:*",
    "@packages/common": "workspace:*",
    "@packages/event-pub-sub": "workspace:*",
    "@packages/redis-connector": "workspace:*",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "glob": "^11.0.0",
    "hono": "^4.6.11",
    "hono-openapi": "^0.4.5",
    "install": "^0.13.0",
    "linkifyjs": "^4.1.3",
    "nanoid": "^3.3.6",
    "openapi-types": "^12.1.3",
    "rxjs": "^7.8.1",
    "swagger-ui-dist": "^5.19.0",
    "typia": "^6.6.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.7",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-patch": "^3.2.1",
    "tsc-watch": "^6.2.1",
    "tsx": "^4.7.1",
    "typescript": "5.6.2"
  }
}
